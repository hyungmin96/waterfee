<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:Android="http://schemas.android.com/apk/res/android">
<item Android:state_pressed="false">
<shape Android:shape="rectangle">
    <corners Android:radius="4dp" />
    <solid Android:color="#0074D1" />
    <stroke
        Android:width="2dip"
        Android:color="#0074D1" />
    <padding
        Android:bottom="4dp"
        Android:left="4dp"
        Android:right="4dp"
        Android:top="4dp" />
</shape>
</item>
<item Android:state_pressed="true">
<shape Android:shape="rectangle">
    <corners Android:radius="4dp" />
    <solid Android:color="#015AA1" />
    <stroke
        Android:width="2dip"
        Android:color="#015AA1" />
    <padding
        Android:bottom="4dp"
        Android:left="4dp"
        Android:right="4dp"
        Android:top="4dp" />
</shape>
</item>
</selector>